{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/enhance.js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/dist/index.html" }
  ],
  "outputDirectory": "dist"
}
